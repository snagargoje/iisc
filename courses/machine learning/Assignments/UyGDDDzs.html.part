      function getPrev(cur) {
        if (cur==syncs) return cur;
        for (s=syncs; s.next && s.next!=cur; ) s=s.next;
        return s;
      }
      function getSync(time) {
        var s = (sync.time > time) ? syncs : sync;
        while (s.next && s.next.time<=time) s=s.next;
        return s;
      }
      function updateSlideWindowHeight() {
        var min_height = $('#slide').height();
        if (this.height > min_height) $('#slide').css('min-height',this.height);
      }
      function setSlideBySync(sync) {
        if (!sync.slide.img.src && !sync.slide.img.height) {
          sync.slide.img = new Image(480,360);
          sync.slide.onload = updateSlideWindowHeight();
          sync.slide.img.src = sync.slide.src;
        } else {
          updateSlideWindowHeight.call(sync.slide.img);
        }
        $('#current_slide').attr('src',sync.slide.src);
        try {
          setFullscreenSlide(sync.slide.src);
        } catch (e) {};
      }
      function setAVTime(time,update_player) {
        try { if (update_player) playerSeek(time); } catch (e) {alert(e);}
        var s = sync;
        try {
            s = getSync(time);
            if (s != sync) {
              sync = s;
              setSlideBySync(sync);
              $(".slide_even").attr('style','background-color: white;');
              $(".slide_odd").attr('style','background-color: #f0f0f0;');
              $("#slide_"+sync.time).attr('style','background-color: #c0c0c0;');
            }
        } catch (e) {alert(e);}
        return s;
      }
      function syncCheckTime(time) {
        var delay = 3000;
        var s = hoverTime ? sync : setAVTime(time);
        if (s.next) delay = s.next.time - time + 1;
        if (delay>3000) delay=3000;
        return delay;
      }
      function preloadAllSlides() {
        for (var s=syncs; s; s=s.next) {
          if (!s.slide.img.src) {
            s.slide.img.onload = updateSlideWindowHeight;
            s.slide.img.src = s.slide.src;
          }
        }
      }
      function clickChooseSlide(time) {
        hoverTime = 0;
        lasttime = -1;
        setAVTime(time,true);
        return false;
      }
      function hoverSlide(time) {
        hoverTime = time+1;
        setAVTime(hoverTime);
      }
      function hoverSlideOut() {
        hoverTime = 0;
        lasttime = -1;
        checkTime();
        return false;
      }

      //$('#current_slide').html('<img src="'+sync.slide.src+'">');
      $(document).ready(function() {
        if (sync.slide) { $('#current_slide').attr('src',sync.slide.src); }
        try { sync = syncs.next; setAVTime(0); } catch (e) {}
      });
  
  
    var lasttime=0;
    var logcd=10;
    function checkTime(time) {
      if (typeof(time)=='undefined') {
        try { 
          time = playerTell(); 
        } catch (e) {
          time = 0.0;
        }
      }
      var delay = 3000;
      //alert('time: ' + time + ' -> ' + lasttime + ' | ' + delay + ',' + currentPosition);
      if (time != lasttime && hoverTime==0) {
        
        try { 
          delay = syncCheckTime(time); 
          if (delay>3000) delay=3000;
        } catch (e) {}
        
        if (lasttime!=-1) { // check if we didn't just get out of mouse-hovering
          if (logcd && !--logcd) $.ajax({url:'/site/ajax/'+'log/video/542/62528/',cache:false,async:true});
        }
        lasttime = time;
      }
      return delay;
    }
    var loopTimer=0;
    var loopCheckTime;
    function loopCheckTime() {
      var delay;
      try { delay = checkTime(); } catch (e) { delay=5000;  }
      try { loopTimer = setTimeout( loopCheckTime, delay); } catch (e) {}
    }
    function init_cinema_mode() {
      $("#cinema_shadow").css("height", $(document).height()).hide();
      $(".cinema_switch").click(function(){
        $("#cinema_shadow").css("height", $(document).height()).toggle();
        if ($("#cinema_shadow").is(":hidden")) {
          $(this).html("Turn off the lights").removeClass("cinema_turned_off");
        } else {
          $(this).html("Turn on the lights").addClass("cinema_turned_off");
        } 
        return false;
      });
    }
    function takeScreenshot() {
